liste_1=(25,4,16,216)
liste_2=(6,11,4.1,0)

kl=map(lambda x,y :x*y ,liste_1,liste_2)
for i in kl:
    print(i)

def karekok(x):
    return x**(1/2)
kl=map(karekok,liste_1)
for i in kl:
    print(i)

