sayı=int(input("Müsteri sayısı"))
müşteri_sirasi=[]
for i in range(1,sayı+1):
    müşteri_sirasi.append(i)
talep=0
istek=[]
while True:
    kim=int(input("kim değiştirmek istiyor"))
    istek.append(kim)

    sıra=(müşteri_sirasi.index(kim))
    müşteri_sirasi[sıra], müşteri_sirasi[sıra-1] = müşteri_sirasi[sıra-1], müşteri_sirasi[sıra]
    print(müşteri_sirasi)
    talep+=1
    if istek.count(kim)>3:
      print("hata")
      break
